<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="true" android:state_pressed="true"
        android:color="@color/pressed_active_navigation_icon"/>
    <item android:state_activated="true" android:state_pressed="false"
        android:color="@color/active_navigation_icon" />
    <item android:state_activated="false" android:state_pressed="true"
        android:color="@color/focused_navigation_icon" />
    <item android:color="@color/inactive_navigation_icon"/>
</selector>